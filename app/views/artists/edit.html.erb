<main>
<div class="form">
  <% @artist.errors.full_messages.each do |message| %>
  <p><%= message %></p>
  <% end %>
  
<%= form_for @artist do |f| %>
  <p><%= f.label :name %></p>
  <p><%= f.text_field :name %></p>
  <p><%= f.label :bio %></p>
  <p><%= f.text_area :bio %></p>
  <p><%= f.label :img_url %></p>
  <p><%= f.text_field :img_url %></p>
  <p><%= f.label :music_url %></p>
  <%= f.text_field :music_url %></p>
</p>
  <%= f.submit %>
  <% end %>
</div>

</main>
